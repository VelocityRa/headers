typedef union _SLIST_HEADER
{
  union
  {
    struct
    {
      U64 Alignment;
      U64 Region;
    }; 
    struct
    {
      struct 
      {
        U64 Depth : 16; 
        U64 Sequence : 48; 
      }; 
      struct 
      {
        U64 Reserved : 4; 
        U64 NextEntry : 60; 
      }; 
    } HeaderX64;
  }; 
} SLIST_HEADER, *PSLIST_HEADER; 

typedef struct _MI_ALIGNED_SLIST
{
  union _SLIST_HEADER SList;
  S32 __PADDING__[12];
} MI_ALIGNED_SLIST, *PMI_ALIGNED_SLIST; 

