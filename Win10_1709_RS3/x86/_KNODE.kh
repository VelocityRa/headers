typedef struct _GROUP_AFFINITY
{
  U32 Mask;
  U16 Group;
  U16 Reserved[3];
} GROUP_AFFINITY, *PGROUP_AFFINITY; 

typedef struct _flags
{
  struct 
  {
    U8 Removable : 1; 
    U8 GroupAssigned : 1; 
    U8 GroupCommitted : 1; 
    U8 GroupAssignmentFixed : 1; 
    U8 Fill : 4; 
  }; 
} flags, *Pflags; 

typedef struct _KHETERO_PROCESSOR_SET
{
  U32 IdealMask;
  U32 PreferredMask;
  U32 AvailableMask;
} KHETERO_PROCESSOR_SET, *PKHETERO_PROCESSOR_SET; 

typedef struct _KNODE
{
  U32 IdleNonParkedCpuSet;
  U32 IdleSmtSet;
  U32 IdleCpuSet;
  U32 DeepIdleSet;
  U32 IdleConstrainedSet;
  U32 NonParkedSet;
  S32 ParkLock;
  U32 Seed;
  U32 SiblingMask;
  union
  {
    struct _GROUP_AFFINITY Affinity;
    struct
    {
      U8 AffinityFill[6];
      U16 NodeNumber;
      U16 PrimaryNodeNumber;
      U8 Stride;
      U8 Spare0;
    }; 
  }; 
  U32 SharedReadyQueueLeaders;
  U32 ProximityId;
  U32 Lowest;
  U32 Highest;
  U8 MaximumProcessors;
  struct _flags Flags;
  U8 Spare10;
  struct _KHETERO_PROCESSOR_SET HeteroSets[5];
  U32 PpmConfiguredQosSets[3];
  S32 __PADDING__[5];
} KNODE, *PKNODE; 

