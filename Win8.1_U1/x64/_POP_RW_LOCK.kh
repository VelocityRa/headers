typedef struct _EX_PUSH_LOCK
{
  union
  {
    struct 
    {
      U64 Locked : 1; 
      U64 Waiting : 1; 
      U64 Waking : 1; 
      U64 MultipleShared : 1; 
      U64 Shared : 60; 
    }; 
    U64 Value;
    void* Ptr;
  }; 
} EX_PUSH_LOCK, *PEX_PUSH_LOCK; 

typedef struct _POP_RW_LOCK
{
  struct _EX_PUSH_LOCK Lock;
  struct _KTHREAD* Thread;
} POP_RW_LOCK, *PPOP_RW_LOCK; 

